#!/usr/bin/env python
# -*- coding: ascii -*-

# Where the .asy files are found
lib_filepath = "C:/Users/oscar/Documents/LTspiceXVII/lib/"

# Can be used to disable all text
display_text = False

# Can be used to disable all text
display_comments = True

# Can be used to remove all nodes
display_nodes = True

# Can be used to remove all flags (except ground-flags)
display_flags = True

# Can be used to remove all component values (ex. "10k" -> "")
display_component_values = True

# Can be used to remove all instance names (ex. "R5" -> "")
display_component_InstName = True

# Can be used to alter the text-size (all text), will ruin alignment
font_scale = 1.00

# The dimensions of the file
svg_file_width = 4000
svg_file_height = 4000

# May remedy clipping
svg_content_border = 20

